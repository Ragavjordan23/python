for star in range(7):
    print("* ",end =' ')
print()
for row in range(7):
    for star in range(7):
        if star!=3:
            print("  ",end =' ')
        else:
            print("* ",end =' ')
    print() 
for star in range(7):
    print("* ",end =' ')
print() 

# output:

*  *  *  *  *  *  *  
         *           
         *           
         *           
         *           
         *           
         *           
         *           
*  *  *  *  *  *  *  

for star in range(7):
    print("* ",end =' ')
print()
for row in range(7):
    for star in range(7):
        if star!=3:
            print("  ",end =' ')
        else:
            print("* ",end =' ')
    print() 
for star in range(2):
    print(" ","*", end =' ')
print() 
# output:

*  *  *  *  *  *  *  
         *           
         *           
         *           
         *           
         *           
         *           
         *           
  *   * 

for star in range(7):
  print("*", end = '\n')
for star in range(7):
  print("*", end = ' ')
print()


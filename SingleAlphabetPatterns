for star in range(6):
  if star!=0:
    print("*", end = ' ')
  else:
    print(" ", end = ' ')
print()
for row in range(7):
  for star in range(7):
    if star==0 or star==6: 
      print("*", end = ' ')
    else:
      print(" ", end = ' ')
  print()
for star in range(6):
  if star!=0:
    print("*", end = ' ')
  else:
    print(" ", end = ' ')
print()
for row in range(7):
  for star in range(7):
    if star==0 or star==6: 
      print("*", end = ' ')
    else:
      print(" ", end = ' ')
  print()
  

  *       *     
    *   *       
      *         

 for row in range(7,-1,-1):
    for star in range(7):
        if star==0 or star==6 or row<=3.5  and row==star or row<=3.5 and star==6-row : 
            print("*", end = ' ')
        else:
            print(" ", end = ' ')
    print()
    
 # output:
 
*           * 
*           * 
*           * 
*           * 
*     *     * 
*   *   *   * 
* *       * * 
*           * 

for row in range(7):
    for star in range(7):
        if  row<=7  and row==star or row<=7 and star==6-row : 
            print("*", end = ' ')
        else:
            print(" ", end = ' ')
    print()
    
# output:

*           * 
  *       *   
    *   *     
      *       
    *   *     
  *       *   
*           * 

for row in range(7):
    for star in range(7):
        if  row<=3.5  and row==star or row<=37 and star==6-row : 
            print("*", end = ' ')
        else:
            print(" ", end = ' ')
    print()
    
# output:

*           * 

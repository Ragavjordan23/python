for row in range(7):
  for star in range(7):
    if star==0 or star==6-row: 
      print("*", end = ' ')
    else:
      print(" ", end = ' ')
  print()
for row in range(7):
  for star in range(7):
    if star==0 or star==row-0: 
      print("*", end = ' ')
    else:
      print(" ", end = ' ')
  print()
  
  # output:
  
*           * 

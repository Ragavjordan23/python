for row in range(7):
    for star in range(7):
        if row==0 and row<=6  and row<=6 or star==6-row or row==6: 
            print("*", end = ' ')
        else:
            print(" ", end = ' ')
    print()
    
# output:

* * * * * * * 
          *   
        *     
      *       
    *         
  *           
